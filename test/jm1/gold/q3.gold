    LEFT JOIN RentalAgreementRentables ON (ReceiptAllocation.RAID=RentalAgreementRentables.RAID AND "2016-03-01" <= RentalAgreementRentables.RARDtStop AND RentalAgreementRentables.RARDtStart < "2016-04-01")
    LEFT JOIN Rentable ON (Rentable.RID=RentalAgreementRentables.RID AND Rentable.RID > 0)
